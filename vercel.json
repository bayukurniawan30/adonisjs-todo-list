{
  "version": 2,
  "buildCommand": "node ace build --ignore-ts-errors",
  "outputDirectory": "/build",
  "devCommand": "node bin/server.js",
  "builds": [
    {
      "src": "build/bin/server.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["build/**"] }
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "build/bin/server.js" }]
}
